<%= render_session_component("front/topics/Index", {
  topics: @topics.map do |topic|
    {
      id: topic.id,
      title: topic.title,
      link: "/topics/#{topic.source_id}",
      created_at: render_time(topic.created_at),
      label_text: topic.label_text
    }
  end,
  pagination: paginate(@topics)
}) %>
